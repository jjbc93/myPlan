{% extends 'layout.html.twig' %}
{% block content %}
    {%for message in app.session.flashbag().get('status')%}
    <div class="alert alert-success">{{message}}</div>
    {%endfor%}
    <div class="col-lg-6">
            <h2>Please</h2>
            {%for event in events%}
            <div class="col-lg-11">
                    <p><img src="{{asset('uploads/'~event.imagen)}}"></p>
                    <img src="{{ asset('favicon.ico') }}" alt="Symfony!" />

            </div>
           

    </div>
    <div class="clearfix"></div>
    {%endfor%}
    {%endblock%}