<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Evento" table="eventos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="descripcion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="horario" type="string" column="horario" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="string" column="fecha" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="organizadores" type="string" column="Organizadores" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lugar" type="string" column="lugar" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
