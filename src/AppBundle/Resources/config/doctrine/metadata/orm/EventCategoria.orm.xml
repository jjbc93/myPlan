<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventCategoria" table="event_categoria">
    <indexes>
      <index name="id_evento" columns="id_evento"/>
      <index name="id_categoria" columns="id_categoria"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idCategoria" target-entity="Categoria" fetch="LAZY">
      <join-columns>
        <join-column name="id_categoria" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEvento" target-entity="Eventos" fetch="LAZY">
      <join-columns>
        <join-column name="id_evento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
