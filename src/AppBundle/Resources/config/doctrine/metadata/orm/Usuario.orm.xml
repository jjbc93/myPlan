<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuarios">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nick" type="string" column="nick" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correo" type="string" column="correo" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rol" type="string" column="rol" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imagen" type="string" column="imagen" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
